{
  "name": "database",
  "version": "0.1.0",
  "private": true,
  "description": "Prisma database schema e client para Bol√£o do Chuveiro Ligado",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "generate": "dotenv -e ../../.env -- prisma generate",
    "push": "dotenv -e ../../.env -- prisma db push",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:prod": "dotenv -e ../../.env -- prisma migrate deploy",
    "studio": "dotenv -e ../../.env -- prisma studio",
    "seed": "dotenv -e ../../.env -- tsx prisma/seed.ts",
    "reset": "dotenv -e ../../.env -- prisma migrate reset",
    "format": "prisma format"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "prisma": "^5.7.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
